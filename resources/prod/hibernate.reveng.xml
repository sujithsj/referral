<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

  <type-mapping>
    <sql-type jdbc-type="DECIMAL" hibernate-type="Double"/>
    <sql-type jdbc-type="INTEGER" hibernate-type="Long"/>
  </type-mapping>


  <table-filter match-name="marketing_material"/>
  <table-filter match-name="marketing_material_type"/>
  
  <table-filter match-name="product"/>
  <table-filter match-name="product_group"/>
  






</hibernate-reverse-engineering>

